{
  "name": "psuvr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "frontend-dev": "vite --host",
    "frontend-build": "vite build",
    "backend-build": "node server.js",
    "backend-dev": "nodemon server.js",
    "dev": "concurrently --kill-others \"npm run frontend-build\" \"npm run backend-dev\"",
    "prod": "concurrently --kill-others \"npm run heroku-postbuild\" \"npm run backend-dev\"",
    "heroku-postbuild": "vite build",
    "ser": "nodemon server.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "gsap": "^3.12.2",
    "include-media": "^2.0.0",
    "nodemon": "^2.0.22",
    "sass": "^1.63.6",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "@google-cloud/speech": "^6.7.0",
    "@google/generative-ai": "^0.20.0",
    "agora-rtc-sdk-ng": "^4.22.1",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "nipplejs": "^0.10.1",
    "shopify-buy": "^2.22.0",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "sweetalert2": "^11.14.2",
    "three": "^0.154.0"
  }
